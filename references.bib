	Keywords = {Magnetic resonance imaging; Multi-modal; Pediatric; Sparse},
	Mesh = {Adolescent; Brain; Brain Mapping; Child; Female; Humans; Image Interpretation, Computer-Assisted; Male; Multimodal Imaging; Principal Component Analysis},
	Month = {Feb},
	Pages = {43-53},
	Pmc = {PMC4329066},
	Pmid = {25448483},
