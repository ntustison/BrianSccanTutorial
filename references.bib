	Owner = {stnava},
	Pages = {142--156},
	Pmid = {17394212},
	Timestamp = {2014.08.11},
	Title = {Modeling low-frequency fluctuation and hemodynamic response timecourse in event-related fMRI.},
	Url = {http://dx.doi.org/10.1002/hbm.20379},
